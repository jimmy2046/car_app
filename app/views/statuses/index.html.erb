<h1>Statuses</h1>
 
<table>
  <tr>    
    <th>My Status</th>
    <th>Created_at</th>      
  </tr>
 
  <% @statuses.each do |status| %>
    <tr>      
      <td><%= status.mystatus %></td>
      <td><%= status.created_at %></td>
    </tr>
  <% end %>
</table>

<br />
<%= link_to 'Update My Status', new_status_path %><br />
<%= link_to 'Go home', root_path %>
